{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.18.0",
    "cleave.js": "1.3.3",
    "moment": "2.22.1",
    "node-sass-chokidar": "1.2.2",
    "npm-run-all": "4.1.3",
    "nuka-carousel": "4.2.0",
    "pusher-js": "4.2.2",
    "react": "^16.3.2",
    "react-big-calendar": "0.19.1",
    "react-big-scheduler": "0.2.0",
    "react-dom": "^16.3.2",
